{
  "functions": {
    "ignore": [
      "node_modules",
      ".git",
      "firebase-debug.log",
      "firebase-debug.*.log"
    ],
    "source": "build/productionBackend",
    "runtime": "nodejs16"
  },
  "hosting": {
    "public": "build/productionWeb",
    "rewrites": [
      {
        "source": "/api/search",
        "function": "search"
      },
      {
        "source": "/api/slack/slashcommand",
        "function": "slackSlashCommand"
      }
    ]
  }
}
